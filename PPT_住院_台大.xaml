eedZero AndAlso result.Length &gt; 0 AndAlso Not result.ToString().StartsWith(&quot;零&quot;) Then&#xA;                result.Insert(0, &quot;零&quot;)&#xA;            End If&#xA;&#xA;            result.Insert(0, partSb.ToString())&#xA;            needZero = False&#xA;        Else&#xA;            ' 這一組是 0000，記一下之後可能要補零&#xA;            If result.Length &gt; 0 AndAlso Not result.ToString().StartsWith(&quot;零&quot;) Then&#xA;                needZero = True&#xA;            End If&#xA;        End If&#xA;&#xA;        n \= 10000&#xA;        bigIndex += 1&#xA;    End While&#xA;&#xA;    out_ChineseText = result.ToString()&#xA;End If&#xA;" DisplayName="Invoke Code" sap:VirtualizedContainerService.HintSize="434,197" sap2010:WorkflowViewState.IdRef="InvokeCode_15">
